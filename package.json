{
  "name": "swapps-finance",
  "private": true,
  "version": "1.0.0",
  "description": "Swapps Finance - The robust Smart Contract for Swapps Finance BEP20 Token.",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dovellous/project-flashloans-uniswap-v3.git"
  },
  "keywords": [
    "flashloans",
    "uniswap"
  ],
  "license": "UNLICENSED",
  "scripts": {
    "start": "npm run dev",
    "dev": "cross-env NODE_ENV=development vite",
    "build": "cross-env NODE_ENV=production vite build && npx workbox generateSW workbox-config.js",
    "build-capacitor-ios": "cross-env NODE_ENV=production vite build && npx cap copy ios",
    "build-capacitor-android": "cross-env NODE_ENV=production vite build && npx cap copy android",
    "postinstall": "cpy ./node_modules/framework7-icons/fonts/*.* ./src/fonts/ && cpy ./node_modules/material-icons/iconfont/*.* ./src/fonts/",
    "web-app": "npm run build && npx cap sync && npx cap run web",
    "ios-app": "npm run build && npx cap sync && npx cap run ios",
    "android-app": "npx cap sync android && npm run build && npx cap copy android && npx cap run android",
    "check-updates-install": "npm install -g npm-check-updates",
    "check-updates": "ncu -u",
    "update": "npm update",
    "react-start": "react-scripts start",
    "react-build": "react-scripts build",
    "react-test": "react-scripts test",
    "react-eject": "react-scripts eject",
    "contract-flatten": "npx truffle-flattener src/blockchain/contracts/token/BEP20/SwappsBEP20.sol > dist/SwappsBEP20.dist.sol",
    "contract-analyze": "npx sol2uml dist/$contract.dist.sol -o analysis/uml/$contract.svg",
    "contract-dev": "lite-server",
    "truffle:console": "truffle develop",
    "truffle:compile": "scripts/compile.sh",
    "truffle:compile-all": "npx truffle compile --all",
    "truffle:test": "npm run truffle:compile && scripts/test.sh",
    "hardhat:console": "hardhat console",
    "hardhat:compile": "hardhat compile",
    "hardhat:test": "hardhat test",
    "hardhat:coverage": "hardhat coverage --network coverage",
    "contract-test": "npm run hardhat:test",
    "coverage": "scripts/coverage.sh",
    "contract-clean": "rm -rf coverage",
    "contract-profile": "npm run clean && npm run coverage && open coverage/index.html",
    "contract-lint": "npm run lint:js && npm run lint:sol",
    "contract-lint:fix": "npm run contract-lint:js:fix",
    "contract-lint:js": "eslint .",
    "contract-lint:js:fix": "eslint . --fix",
    "contract-lint:sol": "solhint --max-warnings 0 \"contracts/**/*.sol\"",
    "flat": "scripts/flat.sh",
    "fix-ps1-error": "Set-ExecutionPolicy -ExecutionPolicy RemoteSigned -Scope LocalMachine"
  },
  "browserslist": [
    "Android >= 7",
    "IOS >= 11",
    "Safari >= 11",
    "Chrome >= 49",
    "Firefox >= 31",
    "Samsung >= 5"
  ],
